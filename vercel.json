{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts",
            "methods": ["POST"],
            "headers": {
                "Acces-Control-Allow-Origin": "*"
            }
        }
    ]
}