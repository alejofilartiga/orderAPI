{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "https://campitoshop.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE, OPTIONS"
            },
            "dest": "index.ts"
        }
    ]
}